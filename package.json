{
  "name": "piggy-proxy",
  "version": "1.0.0",
  "description": "",
  "main": "piggy-proxy.js",
  "scripts": {
    "test": "mocha --reporter spec --recursive ./test",
    "start": "node src/start",
    "start:watch": "nodemon src/start",
    "build:client": "npm-run-all --parallel build:client:javascripts build:client:stylesheets",
    "build:client:watch": "npm-run-all --parallel build:client:javascripts:watch build:client:stylesheets:watch",
    "build:client:javascripts": "browserify src/client/javascripts/main.tsx -p [ tsify  --jsx react] > src/client/javascripts/main.bundle.js",
    "build:client:javascripts:watch": "watchify src/client/javascripts/main.tsx -p [ tsify --jsx react] -o src/client/javascripts/main.bundle.js",
    "build:client:stylesheets": "node-sass src/client/stylesheets/main.scss src/client/stylesheets/main.bundle.css",
    "build:client:stylesheets:watch": "npm run build:client:stylesheets -- --watch",
    "dev": "npm-run-all --parallel build:client:watch start:watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-sass": "^3.3.6",
    "es6-promise": "^3.0.2",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "mobx": "^2.4.1",
    "mobx-react": "^3.5.1",
    "node-uuid": "^1.4.7",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-transition-group": "^15.3.0",
    "react-dom": "^15.2.1",
    "request": "^2.73.0",
    "serve-static": "^1.11.1",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.3.0",
    "tsify": "^1.0.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "watchify": "^3.7.0"
  }
}
